

<div class="row">



  <% @photos.desc.each do |photo| %>
      <figure class="col-md-4" >
          <div class="hovereffect">
            <%= image_tag photo.file, class: "img-fluid", style: "height: 100%", alt: "picture"%>
              <div class="overlay">
                
                 <p>
                  <%= link_to photo.title, photo_path(photo) %>
                </p>
                <% if current_user.id == photo.user_id%>
                  <p>
                    <%= link_to "Edit", edit_photo_path(photo) %>
                  </p>
                  <p>
                    <%= link_to 'Delete', photo, method: :delete, data: {confirm: 'Are you sure?'} %>
                  </p>
                <% end %>
              </div>
              </div> 
        </figure>

  <% end %>

